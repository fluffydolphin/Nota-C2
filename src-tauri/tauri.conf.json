{
    "build": {
        "beforeDevCommand": "",
        "beforeBuildCommand": "",
        "devPath": "../src",
        "distDir": "../src",
        "withGlobalTauri": true
    },
    "package": {
        "productName": "Nota-C2",
        "version": "0.0.0"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "shell": {
                "all": false,
                "open": true
            }
        },
        "bundle": {
            "active": true,
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "com.nota-c2.app",
            "targets": "all"
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [{
            "fullscreen": false,
            "resizable": true,
            "title": "Nota-C2",
            "width": 1280,
            "height": 720
        }]
    }
}